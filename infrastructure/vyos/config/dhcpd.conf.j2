# {{ ansible_managed }}
# This configuration file is generated by Ansible.

option domain-name "{{ domain_name }}";
option domain-name-servers {{ dns_servers|join(', ') }};

default-lease-time 600;
max-lease-time 7200;

subnet {{ lan_subnet }} netmask 255.255.255.0 {
  range {{ lan_start_ip }} {{ lan_stop_ip }};
  option routers {{ lan_gateway }};
}
