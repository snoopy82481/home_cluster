{
  "ansible.ansibleLint.arguments": "-c .github/linters/.ansible-lint",
  "ansible.ansible.path": ".venv/bin/ansible",
  "ansible.python.activationScript": ".venv/bin/activate",
  "ansible.python.interpreterPath": ".venv/bin/python3",
  "ansible.validation.enabled": true,
  "ansible.validation.lint.arguments": "-c ansible/.ansible-lint",
  "ansible.validation.lint.enabled": true,
  "ansible.validation.lint.path": ".venv/bin/ansible-lint",
  "files.associations": {
    "*.json5": "jsonc",
    "**/ansible/**/*.yml": "ansible",
    "**/ansible/**/*.sops.yml": "yaml",
    "**/ansible/**/inventory/**/*.yml": "yaml",
    "**/terraform/**/*.tf": "terraform",
    "**/cluster/**/*.sops.toml": "plaintext",
    "*.env": "dotenv"
  },
  "prettier.configPath": ".ci/prettier/.prettierrc.yaml",
  "prettier.ignorePath": ".github/linters/.prettierignore",
  "explorer.autoReveal": false,
  "editor.fontLigatures": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.hover.delay": 1500,
  "files.trimTrailingWhitespace": true,
  "cSpell.enabled": false,
  "yaml.schemas": {
    "Kubernetes": "cluster/*.yaml",
    "https://raw.githubusercontent.com/recyclarr/recyclarr/master/schemas/config-schema.json": "recyclarr.yaml",
