{
  "ansible.ansibleLint.arguments": "-c .github/linters/.ansible-lint",
  "files.associations": {
    "*.json5": "jsonc",
    "**/ansible/**/*.yml": "ansible",
    "**/ansible/**/*.sops.yml": "yaml",
    "**/ansible/**/inventory/**/*.yml": "yaml",
    "**/terraform/**/*.tf": "terraform",
    "**/cluster/**/*.sops.toml": "plaintext"
  },
  "prettier.configPath": ".github/linters/.prettierrc.yaml",
  "prettier.ignorePath": ".github/linters/.prettierignore",
  "explorer.autoReveal": false,
  "editor.fontLigatures": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs":"active",
  "cSpell.enabled": false,
  "yaml.schemas": {
    "Kubernetes": "cluster/*.yaml",
    "recyclarr": "https://raw.githubusercontent.com/recyclarr/recyclarr/master/schemas/config-schema.json"
  },
  "material-icon-theme.folders.associations": {
    ".taskfiles": "utils",
    "bootstrap": "import",
    "charts": "kubernetes",
    "kube-system": "kubernetes",
    "networking": "connection",
    "monitoring": "event",
    "crds": "ci",
    "flux": "generator",
    "flux-system": "generator",
    "radarr": "video",
    "radarr4k": "video",
    "sonarr": "video",
    "sonarranime": "video",
    "sonarranime-widowmaker": "video",
    "tautulli": "event"
  },
  "conventionalCommits.scopes": [
    "vscode",
    "lidarr",
    "ci",
    "trilio",
    "qbittorrent",
    "k10",
    "flux",
    "ingress-nginx",
    "authentik",
    "node-problem",
    "cluster",
    "language-tools",
    "satisfactory",
    "actions",
    "arc",
    "nzbget",
    "static",
    "blackbox",
    "hs110",
    "hass",
    "openbooks",
    "amd",
    "rook",
    "kps",
    "sonarr",
    "recyclarr",
    "hajimari",
    "cillium",
