{
  "files.associations": {
    "**/*.json5": "json5",
    "**/opentofu/**/*.tf": "terraform",
    "*.env": "dotenv",
    "*.just": "just",
    "Justfile": "just"
  },
  "prettier.configPath": ".ci/prettier/.prettierrc.yaml",
  "prettier.ignorePath": ".github/linters/.prettierignore",
  "explorer.autoReveal": false,
  "editor.fontLigatures": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.hover.delay": 1500,
  "files.trimTrailingWhitespace": true,
  "cSpell.enabled": false,
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    "Kubernetes": "./kubernetes/*.yaml",
    "https://raw.githubusercontent.com/recyclarr/recyclarr/master/schemas/config-schema.json": "./kubernetes/apps/media/recyclarr/app/configs/*.yaml",
    "https://raw.githubusercontent.com/recyclarr/recyclarr/master/schemas/settings-schema.json": "./kubernetes/apps/media/recyclarr/settings.yaml",
    "https://taskfile.dev/schema.json": [
      "**/Taskfile.yaml",
      ".taskfiles/*.yml"
    ],
    "https://raw.githubusercontent.com/budimanjojo/talhelper/master/pkg/config/schemas/talconfig.json": "./talos/talconfig.yaml"
  },
  "material-icon-theme.files.associations": {
    "*.sops.yaml": "lock",
    "helmfile.yaml": "helm",
    "kubeconfig": "kubernetes",
    "talosconfig": "kubernetes"
  },
  "material-icon-theme.folders.associations": {
    // top level
    ".taskfiles": "utils",
    "bootstrap": "seeders",
    "flux": "pipe",
    "talos": "linux",
    // namespaces
    "cert-manager": "guard",
    "kube-system": "kubernetes",
    "networking": "connection",
    "monitoring": "event",
    "flux-system": "pipe",
    "rook-ceph": "dump",
    // apps
    "cloudflared": "cloudflare",
    "radarr": "video",
    "radarruhd": "video",
    "radarranime": "video",
    "sonarr": "video",
    "sonarranime": "video",
    "sonarruhd": "video",
    "tautulli": "event"
  },
  "conventionalCommits.scopes": [
    "ci",
    "actions",
    "tasks",
    "vscode",
    "talos",
    "flux",
    "monitoring",
    "cilium",
    "metrics-server",
    "reloader",
    "alertmanager",
    "grafana",
    "kyverno",
    "cert-manager",
    "external-dns",
    "k8s-gateway",
    "rook-ceph",
    "qbittorrent",
    "bazarr",
    "bazarr-anime",
    "bazarr-uhd",
    "overseer",
    "prowlarr",
    "readarr",
    "radarr",
    "radarr-uhd",
    "radarranime",
    "recyclarr",
    "sabnzbd",
    "node-problem",
    "sonarr",
    "sonarranime",
    "sonarruhd",
    "tautulli",
    "cloudflared",
    "kube-prometheus-stack",
    "huntarr",
    "homepage",
    "tqm",
    "envoy-gateway"
  ],
  "vs-kubernetes": {
    "vs-kubernetes.kubeconfig": "./kubeconfig",
    "vs-kubernetes.knownKubeconfigs": []
  },
  "sops.defaults.ageKeyFile": "age.key",
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv"
}
